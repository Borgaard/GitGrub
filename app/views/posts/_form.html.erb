<%= form_for @post do |f| %>
	<%= f.label :place %>
	<%= f.text_field :place %>
	<br>
	<%= f.label :neighborhood %>
	<%= f.text_field :neighborhood %>
	<br>
	<%= f.label :time %>
	<%= f.time_field :time %>
	<br>
	<%= f.label :body %>
	<%= f.text_field :body %>
	<br>
	<%= f.submit 'Save' %>
<% end %>
